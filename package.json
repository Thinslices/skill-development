{
    "name": "ts-studies",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "SKIP_ENV_VALIDATION=true next build",
        "dev": "npm run prisma:migrations:run && next dev",
        "start": "npm run prisma:migrations:run && next start -p $PORT",
        "lint": "next lint",
        "postinstall": "prisma generate",
        "prisma:migrations:run": "prisma db push",
        "docker:db:build": "docker build -f Dockerfile -t skill-development-db .",
        "docker:db:run": "docker run -d -p 5432:5432 --name skill-development-db skill-development-db",
        "docker:db:init": "npm run docker:db:build && npm run docker:db:run",
        "docker:db:start": "docker start skill-development-db"
    },
    "prettier": {
        "tabWidth": 4,
        "semi": true,
        "bracketSpacing": true,
        "arrowParens": "avoid",
        "endOfLine": "lf"
    },
    "dependencies": {
        "@next-auth/prisma-adapter": "^1.0.5",
        "@prisma/client": "^4.9.0",
        "@tanstack/react-query": "^4.20.0",
        "@trpc/client": "^10.9.0",
        "@trpc/next": "^10.9.0",
        "@trpc/react-query": "^10.9.0",
        "@trpc/server": "^10.9.0",
        "classnames": "^2.3.2",
        "dotenv": "^16.0.3",
        "next": "13.1.6",
        "next-auth": "^4.19.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-table": "^7.8.0",
        "superjson": "1.9.1",
        "zod": "^3.20.2",
        "zustand": "^4.3.3"
    },
    "devDependencies": {
        "@types/node": "^18.11.18",
        "@types/prettier": "^2.7.2",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.10",
        "@types/react-table": "^7.7.14",
        "@typescript-eslint/eslint-plugin": "^5.47.1",
        "@typescript-eslint/parser": "^5.47.1",
        "autoprefixer": "^10.4.7",
        "eslint": "^8.30.0",
        "eslint-config-next": "13.1.6",
        "postcss": "^8.4.14",
        "prettier": "^2.8.4",
        "prettier-plugin-tailwindcss": "^0.2.2",
        "prisma": "^4.9.0",
        "tailwindcss": "^3.2.0",
        "typescript": "^4.9.4"
    },
    "ct3aMetadata": {
        "initVersion": "7.4.0"
    },
    "engines": {
        "npm": ">=8.0.0",
        "node": ">=16.0.0"
    }
}
